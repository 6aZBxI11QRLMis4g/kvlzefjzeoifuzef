        	function validate(){

    			var form = document.contactform;
				apos=form.ctf_mail.value.indexOf("@")
				dotpos=form.ctf_mail.value.lastIndexOf(".")

				if (form.ctf_name.value=="" || form.ctf_message.value==""){
    	   			alert("Geef aub minstens uw naam , E-mail Adres en een korte Omschrijving in!");
        		}
				else if(form.ctf_mail.value == "" || apos<1 || dotpos-apos<2){
						alert("Vul je email in.");
				}
           		else{
    	    		form.submit();
    	    	}
    	  	}
        </script>
        
        
