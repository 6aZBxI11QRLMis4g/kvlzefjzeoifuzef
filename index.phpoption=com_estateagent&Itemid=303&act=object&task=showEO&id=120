<script src="http://maps.google.com/maps?file=api&amp;v=2&amp;key=ABQIAAAAAbiBtIk1kuCDTTqeka1DVhR6CBAZFUjKMR2Jue5q2To4uHUSIBRwEIqB6n5lTsseAtGxTVD8LEZFxg" type="text/javascript"></script>
<script type="text/javascript">
//------------------------------------------
// GoogleMap api written for Estate Aagent Improved
// 2007 Manos Orfanoudakis
// http://www.eaimproved.eu
// Published under GNU GPL
//------------------------------------------
//<![CDATA[
var eagmap;
var geocoder = new GClientGeocoder();

	function showLocation(point){
		var parts=point.split(";");
		var lat = parseFloat( parts[0] );
	    var lon = parseFloat( parts[1] );
		lat = lat.toFixed(6);
		lon = lon.toFixed(6);
		var point=new GLatLng(lat,lon);
		eagmap.checkResize();
		eagmap.setCenter(point);
		eagmap.setZoom(parseInt(parts[2]));

	}
		
	function addPoint(point, text){
		var parts=point.split(";");
		var lat = parseFloat( parts[0] );
	    var lon = parseFloat( parts[1] );
		lat = lat.toFixed(6);
		lon = lon.toFixed(6);
		var point=new GLatLng(lat,lon);
		var marker = new GMarker(point);
		eagmap.checkResize();
		eagmap.addOverlay(marker);
		if(text){
			GEvent.addListener(marker, 'click',function(){
				marker.openInfoWindowHtml(text);			
			});
			
		}
		
		
	
	}
	function GMapload() {
		if (GBrowserIsCompatible()&&!eagmap) {
			eaGMapBeforeLoad();
			eagmap = new GMap2(document.getElementById("gmap"));
		    eagmap.setCenter(new GLatLng(0,0), 10);
			eagmap.addControl(new GLargeMapControl());
			eagmap.setMapType(G_HYBRID_MAP);
			eagmap.addControl(new GMapTypeControl());
			//addPoint("$point");
			fixView();
			showLocation("45.143305;13.607941;10");
			eaGMapAfterLoad();
    	}
    }

function fixView(){
	eagmap.savePosition();
	eagmap.checkResize();
	eagmap.returnToSavedPosition();
	setTimeout('fixView()',1500);
}

	if (window.addEventListener) //DOM method for binding an event
	{
		window.addEventListener("unload", GUnload, false);
		window.addEventListener("load", GMapload, false);
	}
	else if (window.attachEvent) //IE exclusive method for binding an event
	{
		window.attachEvent("onunload", GUnload);
		window.attachEvent("onload", GMapload);
	}	
//]]>
</script>
<script type="text/javascript">
		
		function eaGMapBeforeLoad(){;}
		function eaGMapAfterLoad(){
					addPoint("45.143305;13.607941;10","Vakantiecomplex Belvedere - Vrsar");
				}
		</script>
